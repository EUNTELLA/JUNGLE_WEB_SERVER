{
  "configurations": [
    {
      "name": "windows-gcc-x86",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "defines": [
        "_GNU_SOURCE",
        "_DEFAULT_SOURCE",
        "_POSIX_C_SOURCE=200809L"
      ],
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "windows-gcc-x86",
      "compilerPath": "C:/MinGW/bin/gcc.exe"
    }
  ],
  "version": 4
}